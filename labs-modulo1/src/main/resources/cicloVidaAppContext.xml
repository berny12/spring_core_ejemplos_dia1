<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="beanConMetodoInit" class="com.synergyj.cursos.spring.beans.ciclovida.BeanConMetodoInit"
		init-method="init" destroy-method="destroy">
		<property name="edad" value="67" />
		<property name="nombre" value="Juan Perez" />
	</bean>

	<bean id="beanInvadido" class="com.synergyj.cursos.spring.beans.ciclovida.BeanInvadidoPorSpring">
		<property name="nombre" value="Invadido" />
	</bean>


	<bean class="com.synergyj.cursos.spring.beans.ciclovida.BeanConAnotaciones" />
</beans>



